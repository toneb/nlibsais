<?xml version="1.0" encoding="utf-8"?>
<configuration>

    <packageSources>
        <clear />
        <add key="nuget" value="https://api.nuget.org/v3/index.json" />
        <add key="nuget-local" value="packages" />
    </packageSources>

    <packageSourceMapping>
        <packageSource key="nuget">
            <package pattern="*" />
        </packageSource>
        <packageSource key="nuget-local">
            <package pattern="NLibSais.*" />
        </packageSource>
    </packageSourceMapping>

</configuration>